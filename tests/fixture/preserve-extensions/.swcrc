{
  "$schema": "http://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false,
    "experimental": {
      "plugins": [
        ["swc-plugin-allow-importing-ts-extensions", { "preserveImportExtension": true }]
      ]
    }
  },
  "isModule": true
}